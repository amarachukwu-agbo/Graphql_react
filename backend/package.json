{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "debug": "nodemon -e js,graphql --exec babel-node --inspect src/index.js",
    "test": "jest",
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "deploy": "prisma deploy --env-file variables.env --force",
    "playground": "graphql playground --dotenv variables.env",
    "dev": "npm-run-all --parallel debug playground "
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.6.5",
    "dotenv": "6.0.0",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.7",
    "graphql-yoga": "1.16.2",
    "jsonwebtoken": "9.0.0",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "prisma": "1.17.1",
    "prisma-binding": "2.1.6",
    "stripe": "^6.12.1"
  },
  "license": "MIT",
  "repository": {
    "url": "wesbos.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4"
  }
}
